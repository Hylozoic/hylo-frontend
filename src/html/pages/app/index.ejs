<%- include('../_header'); %>

<div id="initializing-panel">
  <div class="spinner">
    <div class="double-bounce1"></div>
    <div class="double-bounce2"></div>
  </div>
</div>

<div id="content-outer">

  <div ng-show="loginMode" id="login-holder">
    <div class="container">
      <div class="row">
        <div class="col-md-6">
          <h2>You must be logged in to view this page.</h2>
          <a href="/login">Log in here</a>.
        </div>
      </div>
    </div>
  </div>

  <div ng-hide="loginMode" id="content">
    <ng-include src="'/ui/app/nav.tpl.html'"></ng-include>
    <div ui-view class="container-fluid"></div>
  </div>

  <ng-include src="'/ui/app/menu.tpl.html'"></ng-include>

</div>
<div growl></div>

<script type="text/javascript" src="<%= process.env.JS_BUNDLE_URL %>"></script>

<script>
  filepicker.setKey(hyloEnv.filepicker.key);
</script>

<%- include('../_footer'); %>